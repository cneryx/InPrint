{
  "name": "InPrint",
  "description": "Kill me",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "InPrint"
  },
  "permissions": [
      "http://127.0.0.1:3000/",
      "tabs",
      "webRequest"
  ],
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }]
}